<div class="row">
  <div class="col-md-4">
    <div class="upload-form">
      <%= form_tag({:controller=>"images",:action => 'create'},:id=>"upload-data-form",:multipart => true,:class=>"form-inline") do %>
        <%= file_field 'upload', 'imagefile',:class=>"form-control", accept:'image/*' %>
        <%= submit_tag "Upload", :class => "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
<% if @images.any? %>
  <h4>Images uploaded in the system</h4>
  <div class="row">
    <% @images.each do |image| %>
      <div class="col-md-4">
        <img class="img-thumbnail" src=<%= image.public_url %> />
      </div>
    <% end %>
  </div>
<% end %>
<% if @images.next_page? %>
  <div class="form-row">
    <%= link_to 'Next', root_path(marker: @images.last.object.key), class: "btn btn-default" %>
  </div>
<% end %>